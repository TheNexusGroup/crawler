{
    "name": "crawler-test/php-project",
    "description": "PHP test project for dependency crawler",
    "type": "project",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.3",
        "doctrine/orm": "^2.15",
        "psr/log": "^3.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-symfony": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "CrawlerTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrawlerTest\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}